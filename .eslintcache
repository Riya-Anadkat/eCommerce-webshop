ypography } from '@material-ui/core';\nimport { ShoppingCart } from '@material-ui/icons';\n//import classes from '*.module.css';\nimport useStyles from './styles';\nimport { Link, useLocation } from 'react-router-dom';\n\nimport logo from '../../assets/logo.png'\n\nconst Navbar = ({ totalItems }) => {\n    const classes = useStyles();\n    const location = useLocation();\n\n    return (\n        <>\n            <AppBar position= \"fixed\" className={classes.appBar} color='inherit'>\n                <Toolbar>\n                    <Typography component={Link} to=\"/\" variant=\"h5\" className={classes.title} color=\"inherit\">\n                        <img src={logo} alt=\"Commerce.js\" height=\"45px\" className={classes.image} />\n                            the eyeA store \n                    </Typography>\n                    <div className={classes.grow} />\n                    {/* button is only shown when the user is in on the homepage */}\n                    {location.pathname === '/' && (\n                    <div className={classes.button}>\n                        <IconButton component={Link} to=\"/cart\" aria-label=\"Show cart items\" color=\"inherit\" fontSize=\"large\" >\n                            <Badge badgeContent={totalItems} color= \"secondary\">\n                                <ShoppingCart />\n                            </Badge>\n                        </IconButton>\n                    </div> )}\n\n                </Toolbar>\n            </AppBar>\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/riya/Downloads/VSCode Projects/ecommerce_webshop/src/components/Navbar/styles.js",[],"/Users/riya/Downloads/VSCode Projects/ecommerce_webshop/src/components/index.js",[],"/Users/riya/Downloads/VSCode Projects/ecommerce_webshop/src/components/Products/styles.js",[],"/Users/riya/Downloads/VSCode Projects/ecommerce_webshop/src/lib/commerce.js",[],"/Users/riya/Downloads/VSCode Projects/ecommerce_webshop/src/components/Cart/Cart.jsx",["87","88"